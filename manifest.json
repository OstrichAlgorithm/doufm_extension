{
     "manifest_version": 2,
    "name": "doufm_extension",
    "version": "1.0",
    "description": "doufm的chrome插件",
    "icons": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/19.png",
            "38": "images/38.png"
        },
     "default_popup": "popup.html"
    },
     
    "background": {
        "page": "audio.html"
        
    },
    "permissions": [
        "http://doufm.info/*",
         "downloads"
    ]
    
  
}